    "@vercel/ncc": "^0.38.1",
    "cheerio": "1.0.0-rc.12",
    "dotenv": "^16.0.1",
    "got-scraping": "^3.2.12",
    "got": "^12.1.0",
    "yaml": "^2.1.1"
  }
}
